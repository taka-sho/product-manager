{
  "extends": [
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rulesDirectory": ["tslint-plugin-prettier"],
  "rules": {
    "no-console": [true, "log"],
    "jsx-no-lambda": false,
    "prettier": true
  },
  "linterOptions": {
    "exclude": [
      "**/node_modules/**/*.ts"
    ]
  }
}
